{
  "expo": {
    "name": "Plantor-App",
    "slug": "Plantor-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.skrispyb.PlantorApp"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "WMDD Term 4 Capstone Project at Langara College, Vancouver, Fall 2021.",
    "githubUrl": "https://github.com/PlantorApp/React-Native-App"
  }
}
